<!DOCTYPE html>
<html lang="en">
<head>
	<title>Observations</title>
	<meta charset="UTF-8">
</head>
<body>
	<h1>Observations</h1>
	<ul>
		<% obs.forEach(function(r) { %>
		<li><%= r.tasks_id %> for <%= r.duration.minutes || "00" %>:<%= r.duration.seconds || "00" %></li>
		<% }); %>
	</ul>
</body>
</html>
